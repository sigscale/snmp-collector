[{snmp_collector,
      [{manager_ports, [%MANAGER_PORT%]},
      {queue_files, 15},
      {queue_size, 1048576}]},
{inets,
      [{services,
         [{httpd,
            [{server_name, "rest"},
               {bind_address, "localhost"},
               {directory_index, ["index.html"]},
               {directory, {"/",
                     [{auth_type, mnesia},
                     {auth_name, "snmp.sigscale.org"},
                     {require_group, ["all"]}]}},
              {modules, [mod_alias, mod_auth, mod_responsecontrol,
                  snmp_collector_rest_accepted_content,
                  snmp_collector_rest_get,
                  mod_disk_log, mod_get]},
               {port, %HTTP_PORT%},
               {socket_type, {essl,
                     [{certfile, "%CERT_DIR%/cert.pem"},
                     {keyfile, "%CERT_DIR%/key.pem"}]}},
               {server_root, "%SERVER_ROOT%"},
               {document_root, "%APP_PRIV%/www"}]}]}]}].
